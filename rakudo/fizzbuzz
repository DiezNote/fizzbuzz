#! /usr/bin/env perl6

use v6;

sub fizz-buzz($n) {
    my Str $result = "";
    unless ($n % 3) {
	$result = "Fizz";
    }
    unless ($n % 5) {
	$result ~= "Buzz";
    }

    $result ||= $n.Str;

    return $result;
}

sub MAIN(Int $max = 100) {
    my @generator = (0..*).map(&fizz-buzz);

    .say for @generator[1..$max];
}
